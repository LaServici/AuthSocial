{
    "name": "laservici/authsocial",
    "description": "The package contains the OAuth libraries for the Laravel script.",
    "keywords": ["oauth", "laravel", "laservici"],
    "license": "MIT",
    "authors": [
        {
            "name": "Silivestru Sandel",
            "email": "ssflorin35@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~5.0|~6.0",
        "illuminate/contracts": "~5.0",
        "illuminate/http": "~5.0",
        "illuminate/support": "~5.0",
        "league/oauth1-client": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Laservici\\Authsocial\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
